<Window x:Class="IdeAteProto.PayTogether"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:IdeAteProto"
        mc:Ignorable="d"
        Title="PayTogether" Height="450" Width="800">
    <Grid>
        <Label x:Name="PayTogether" Content="Will you be paying together?" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="266,138,0,0" Height="56" Width="267" FontSize="20"/>
        <Button Content="Yes" HorizontalAlignment="Left" Height="52" Margin="278,226,0,0" VerticalAlignment="Top" Width="92" Background="#FF7AC367"/>
        <Button Content="No" HorizontalAlignment="Left" Height="52" Margin="431,226,0,0" VerticalAlignment="Top" Width="92" Background="#FFE77878"/>
    </Grid>
</Window>
