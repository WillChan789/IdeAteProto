<Window x:Class="IdeAteProto.PayTogether"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:IdeAteProto"
        mc:Ignorable="d"
        Title="PayTogether" Height="450" Width="800">
    <Grid>
        <Label x:Name="AskPay" Content="Will you be paying together?" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="259,119,0,0" Height="56" Width="278" FontFamily="Century" FontSize="20"/>
        <Button Content="Yes" HorizontalAlignment="Left" Height="67" Margin="261,207,0,0" VerticalAlignment="Top" Width="103" FontFamily="Century" FontSize="16" Background="#FF94F159" Click="Button_Click"/>
        <Button Content="No" HorizontalAlignment="Left" Height="67" Margin="433,207,0,0" VerticalAlignment="Top" Width="104" FontFamily="Century" FontSize="16" Background="#FFED8888" Click="Button_Click_1"/>
    </Grid>
</Window>
